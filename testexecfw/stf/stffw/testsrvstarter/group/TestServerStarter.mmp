#include <platform_paths.hrh>

SMPSAFE
TARGET          TestServerStarter.exe
TARGETTYPE      exe

CAPABILITY	ALL -TCB
VENDORID 	0x101FB657
SECUREID        0x102073E4

/* Include paths */
USERINCLUDE     ../../../inc

OS_LAYER_SYSTEMINCLUDE

/* Source files */
SOURCEPATH      ../src

SOURCE          TestServerStarter.cpp

/* Used libraries */
LIBRARY         euser.lib
LIBRARY         stiftestserver.lib

/* Test Server requires large stack */
EPOCSTACKSIZE   40960

// End of File
